<?php

namespace DummyNamespace;

use App\DummyModel;
use Slexx\LaravelActions\Action;

class DummyAction extends Action
{
    /**
     * @param int $id
     * @return bool
     */
    public function execute(int $id)
    {
        $dummyModel = DummyModel::findOrFail($id);

        $dummyModel->delete();

        return true;
    }
}
